{
    "name": "delboy1978uk/bone-user",
    "description": "Integration of delboy1978uk/user for Bone Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Stephen McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
        "delboy1978uk/bone-mail": "^v1.3",
        "delboy1978uk/bone-http": "^2.4",
        "delboy1978uk/bone-doctrine": "^2.0",
        "delboy1978uk/bone-paseto": "^1.0",
        "delboy1978uk/bone-passport": "^1.0",
        "delboy1978uk/form": "^2.5",
        "delboy1978uk/user": "^4.7",
        "laminas/laminas-diactoros": "^3.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "delboy1978uk/dev-tools": "^1.1",
        "delboy1978uk/session": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Bone\\User\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bone\\Test\\User\\": "tests/unit/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": false
        }
    }
}
